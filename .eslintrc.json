{
	"extends": [
		"next/core-web-vitals",
		"prettier",
		"plugin:tailwindcss/recommended",
	],
	"overrides": [
		{
			"files": [
				"*.ts",
				"*.tsx",
				"*.js"
			],
			"parser": "@typescript-eslint/parser"
		}
	],
	"rules": {
		"no-unused-vars": "warn",
		"no-unused-expressions": "warn",
		"no-unused-labels": "warn"
	}
}
