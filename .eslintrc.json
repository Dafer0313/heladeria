{
    "extends": [
        "next/core-web-vitals",
        "prettier",
        "plugin:tailwindcss/recommended",
    ],
    "overrides": [
        {
            "files": [
                "*.js",
				"*.jsx"
            ]
        }
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-unused-expressions": "warn",
        "no-unused-labels": "warn"
    }
}